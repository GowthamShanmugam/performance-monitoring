LoadPlugin swap

<Plugin "swap">
  ValuesPercentage true
  ReportBytes true
  ValuesAbsolute true
</Plugin>

<Plugin "aggregation">
  <Aggregation>
    Plugin "swap"
    Type "swap"

    GroupBy "Host"

    CalculateSum true
  </Aggregation>
</Plugin>

<Plugin "threshold">
    <Plugin "swap">
       <Type "percent">
         Instance used
         FailureMax {{ Failure }}
         WarningMax {{ Warning }}
       </Type>
    </Plugin>
</Plugin>

