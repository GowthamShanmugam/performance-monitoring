<Plugin "python">
    ModulePath "/usr/lib64/collectd/"

    Import "glusterfs_utilization"

    <Module "glusterfs_utilization">
        cluster_id "{{ cluster_id }}"
    </Module>
</Plugin>

<Plugin "threshold">
    <Plugin "cluster_utilization">
       <Type "percent">
         Instance percent_bytes
         FailureMax {{ Failure }}
         WarningMax {{ Warning }}
       </Type>
    </Plugin>
</Plugin>
